<?xml version="1.0" encoding="UTF-8"?>
<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:maps="clr-namespace:Maui.GoogleMaps;assembly=Maui.GoogleMaps"
    x:Class="MauiGoogleMapSample.MapIdPage"
    Title="MapId Page">
    <ContentPage.Content>
        <Grid>
            <maps:Map MapId="{OnPlatform Android='', iOS=''}"
                      InitialCameraUpdate="35.71, 139.81, 8, 30, 0" />
            <Label TextColor="Black" 
                   VerticalOptions="Center" 
                   Text="Please put your own MapIds and recompile" />
        </Grid>
    </ContentPage.Content>
</ContentPage>